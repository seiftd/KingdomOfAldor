{
  "name": "KingdomOfAldoria",
  "version": "1.0.0",
  "description": "Epic 2D Mobile RPG Adventure for Android and iPhone",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "xcodebuild -workspace ios/KingdomOfAldoria.xcworkspace -scheme KingdomOfAldoria -configuration Release -destination generic/platform=iOS archive"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-vector-icons": "^10.0.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-svg": "^13.14.0",
    "react-native-reanimated": "^3.5.4",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-canvas": "^0.1.38",
    "react-native-game-engine": "^1.2.0",
    "react-native-sound": "^0.11.2",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-push-notification": "^8.1.1",
    "react-native-async-storage": "@react-native-async-storage/async-storage",
    "react-native-device-info": "^10.11.0",
    "react-native-orientation-locker": "^1.5.0",
    "react-native-keep-awake": "^4.0.0",
    "react-native-safe-area-context": "^4.7.4",
    "react-navigation": "^4.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "react-native",
    "mobile-game",
    "2d-game",
    "rpg",
    "android",
    "ios",
    "kingdom-aldoria"
  ],
  "author": "Kingdom of Aldoria Team",
  "license": "MIT"
}